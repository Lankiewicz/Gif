{
  "name": "learning-git-2",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "dependencies": {
    "babel-standalone": "^6.26.0",
    "browser-sync": "^2.23.6"
  },
  "devDependencies": {
    "autoprefixer-cli": "^1.0.0",
    "babel-cli": "^6.26.0",
    "babel-preset-latest": "^6.24.1",
    "globstar": "^1.0.0",
    "http-server": "^0.11.1",
    "jshint": "^2.9.5",
    "node-sass": "^4.7.2",
    "npm-run-all": "^4.1.2",
    "nu-html-checker": "^0.1.0",
    "onchange": "^3.3.0"
  },
  "scripts": {
    "test": "npm-run-all test:*",
    "test:html": "globstar nu-html-checker *.html",
    "test:js": "globstar jshint js/*.js",
    "init-project": "npm install && npm-run-all init:*",
    "init:gitignore": "curl https://raw.githubusercontent.com/github/gitignore/master/Node.gitignore -o .gitignore",
    "watch": "npm-run-all build -p watch:*",
    "watch:sass": "node-sass --output-style expanded --source-map true -o css sass --watch",
    "watch:browsersync": "browser-sync start --server --files '*.css' '*.html'",
    "start": "babel script.js --watch --out-file script-compiled.js",
    "watch:autoprefixer": "onchange -d 100 css/style.css -- autoprefixer-cli -o css/style.prefixed.css css/style.css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Lankiewicz/learning-git-2.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Lankiewicz/learning-git-2/issues"
  },
  "homepage": "https://github.com/Lankiewicz/learning-git-2#readme"
}
